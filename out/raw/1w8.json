      "wasPublished": true,
      "type": "wiki",
      "title": "Learning representations",
      "clickbait": "",
      "textLength": 6178,
      "alias": "Learning_representations",
      "externalUrl": "",
      "sortChildrenBy": "likes",
      "hasVote": false,
      "voteType": "",
      "votesAnonymous": false,
      "editCreatorId": "3",
      "editCreatedAt": "2016-02-24 22:56:56",
